# AxyN Mobile Environment Variables
# Copy this file to .env and fill in your actual values

# Privy Authentication
# Get your credentials from https://dashboard.privy.io → Your App → Settings
PRIVY_APP_ID=your_privy_app_id_here
PRIVY_CLIENT_ID=your_privy_client_id_here
PRIVY_CLIENT_SECRET=your_privy_client_secret_here

# AxyN Backend API
# For local development: http://localhost:3000
# For production: https://api.axyn.ai
API_BASE_URL=http://localhost:3000

# Solana Network Configuration
# Options: 'mainnet-beta' or 'devnet'
SOLANA_NETWORK=devnet

# Solana RPC URL
# For mainnet: https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY
# For devnet: https://api.devnet.solana.com
HELIUS_RPC_URL=https://api.devnet.solana.com

